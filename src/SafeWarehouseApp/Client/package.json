{
  "scripts": {
    "styles:prod": "cross-env NODE_ENV=production postcss tailwind.css -o wwwroot/styles.css",
    "styles:dev": "cross-env NODE_ENV=development postcss tailwind.css -o wwwroot/styles.css",
    "build": "copyfiles -u 1 \"./node_modules/alpinejs/dist/alpine.js\" \"./node_modules/idb/build/iife/index-min.js\" \"./node_modules/interactjs/dist/interact.min.js\" \"./wwwroot/js/lib\"",
    "build:css:print": "cross-env NODE_ENV=production tailwind build ./tailwind.css -o ./wwwroot/css/tailwind.print.css"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "alpinejs": "^2.8.0",
    "autoprefixer": "^10.2.3",
    "html-to-pdf-js": "^0.9.3",
    "idb": "^6.0.0",
    "interactjs": "^1.10.2",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "copyfiles": "^2.4.1"
  }
}
